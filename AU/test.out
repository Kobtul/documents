% Collision axioms 
fof( collision_01, axiom, ( ![T]:(?[X,Y]:( collision(succ(T),01) <=> ( (ishere(T,a1,X) & signal(T,a1)) & (ishere(T,01,Y) & ~signal(T,b)) ) )) )).
fof( collision_02, axiom, ( ![T]:(?[X,Y]:( collision(succ(T),02) <=> ( (ishere(T,a2,X) & signal(T,a2)) & (ishere(T,02,Y) & signal(T,b)) ) )) )).
