fof( signal_a3, axiom, ( ![T,Y0,Y1,Y2,Y3,Y4,Y5,Y6]:( signal(T,a3) <=> (~ishere(T,04,Y0) & ~ishere(T,11,Y1) & ~ishere(T,08,Y2) & ~ishere(T,03,Y3) & ~ishere(T,01,Y4) & ~ishere(T,02,Y5) & ~ishere(T,07,Y6)) ) )).
