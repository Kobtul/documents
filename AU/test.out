fof( signal_b2, axiom, ( ![T,X]:(ishere(T,03,X) => signal(T,b2)) )).
fof( signal_c1, axiom, ( ![T]:( signal(T,c1) ) )).
fof( signal_d2_e2, axiom, ( ![T]:(ishere(T,06,e2) => signal(T,d2)) )).
